{
  "name": "ember-lifecycle-component",
  "version": "1.0.25",
  "description": "A component with additional lifecycles for times when you may need need a template.",
  "keywords": [
    "ember-addon",
    "component-manager"
  ],
  "repository": "https://github.com/NullVoxPopuli/ember-lifecycle-component",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:types": "yarn tsc --noEmit --skipLibCheck",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:try-one": "ember try:one",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-typescript": "^4.2.1"
  },
  "devDependencies": {
    "ember-template-lint": "^3.4.2",
    "eslint": "^7.28.0",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.8",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/ember": "^3.16.5",
    "@types/ember-qunit": "^3.4.13",
    "@types/ember__test-helpers": "^2.0.0",
    "@types/qunit": "^2.11.1",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.11.3",
    "ember-cli": "~3.26.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.27.5",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "qunit": "^2.16.0",
    "qunit-dom": "^1.6.0",
    "semantic-release": "17.4.4",
    "typescript": "^4.3.3"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "changelog": {
    "repo": "NullVoxPopuli/ember-lifecycle-component",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal"
    }
  },
  "volta": {
    "node": "14.17.1",
    "yarn": "1.22.10"
  }
}
